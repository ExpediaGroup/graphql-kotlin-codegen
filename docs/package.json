    "@docusaurus/types": "3.3.2",
    "@mdx-js/react": "3.0.1",
    "clsx": "2.1.1",
    "jsdoc-to-markdown": "8.0.1",
    "prism-react-renderer": "2.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-loadable": "5.5.0"
  }
}
